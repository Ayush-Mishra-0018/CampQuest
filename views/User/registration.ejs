<% layout('layouts/boiler') %>
<form action="/register" method="post" class="register-form">
    <input type="email" placeholder="Enter The Email" name="email" required  autocomplete="off">
    <input type="text" placeholder="Enter The UserName" name="username" required autocomplete="off">
    <input type="password" placeholder="Enter The Password" name="password" required >
    <button>Submit</button>
    <a href="/campgrounds" class="secondary-btn-link">Back To Home</a>
</form>

